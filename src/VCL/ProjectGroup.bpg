PROJECTS = dBASElibrary.DLL dBASElibrary.DEU
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

dBASElibrary.DLL: dBASElibrary.dpr
  $(DCC)

dBASElibrary.DEU: DEU\dBASElibrary.dpr
  $(DCC)


